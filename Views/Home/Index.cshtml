@model Chef
<div class="container">
    <h1>Add a Chef!</h1>
    <a href="">Home</a>
    <a href="add_dish">Add Dish</a>
    <form action="createChef" method="post">
        <div class="form-group">
            <span asp-validation-for="f_name"></span>
            <label asp-for="f_name"></label>
            <input type="text" asp-for="f_name">
        </div>
        <div class="form-group">
            <span asp-validation-for="l_name"></span>
            <label asp-for="l_name"></label>
            <input type="text" asp-for="l_name">
        </div>
        <div class="form-group">
            <span asp-validation-for="dob"></span>
            <label asp-for="dob"></label>
            <input type="date" asp-for="dob">
        </div>
        
        <input type="submit" value="Add Chef">
    </form>

</div>